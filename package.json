{
  "name": "liff-sample",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sls": "node --max-old-space-size=4096 --expose-gc --inspect node_modules/serverless/bin/serverless",
    "build": "npm run sls -- webpack",
    "local": "npm run sls -- offline --stage local -P 3001",
    "nuxt": "export STAGE=local && nuxt-ts",
    "nuxt:gen": "nuxt-ts generate",
    "deploy:dev": "export STAGE=dev && export AWS_SHARED_CREDENTIALS_FILE=$(pwd)/.aws/credentials && export AWS_REGION=$(aws configure get region --profile $STAGE) && npm run nuxt:gen && npm run sls -- deploy",
    "deploy:prod": "export STAGE=prod && export AWS_SHARED_CREDENTIALS_FILE=$(pwd)/.aws/credentials && export AWS_REGION=$(aws configure get region --profile $STAGE) && npm run nuxt:gen && npm run sls -- deploy --stage prod",
    "lint": "eslint --ext .ts,.js,.vue ./src",
    "fix": "eslint --ext .ts,.js,.vue ./src --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
